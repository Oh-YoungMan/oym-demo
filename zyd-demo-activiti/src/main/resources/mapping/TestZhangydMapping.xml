<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.zyd.activiti.dao.TestZhangydDao">

    <select id="getById" resultType="com.cn.zyd.activiti.entity.TestZhangyd">
        select * from act_ge_bytearray where id = #{id}
    </select>

    <insert id="insert" parameterType="com.cn.zyd.com.cn.zyd.activiti.entity.ActGeBytearray">
        insert into act_ge_bytearray (
            id,
            rev,
            name,
            deploymentId,
            bytes,
            generated
        )values(
            #{id},
            #
        )
    </insert>

</mapper>